net: "cnn.prototxt"
solver_type: SGD
base_lr: 0.03
lr_policy: "step"
gamma: 0.9
stepsize: 500 ## learning rate update: base_lr *= gamma,  in every stepsize calcuration
max_iter: 5000
momentum: 0.9
display: 1
snapshot_prefix: "cnn"
